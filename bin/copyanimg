#!/bin/sh

#Copy all image to current dir

if [ ! "$TOP" ]; then
  echo "Couldn't locate the top of the 8909"
  exit 1
fi

echo "Copying image ......"

cp -p ${OUT}/emmc_appsboot.mbn .
cp -p ${OUT}/boot.img .
cp -p ${OUT}/system.img .
cp -p ${OUT}/userdata.img .
cp -p ${OUT}/recovery.img .
cp -p ${OUT}/persist.img .
cp -p ${OUT}/cache.img .
